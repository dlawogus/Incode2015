<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.woody.ext.incode2015" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lc"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- Splash Activity -->
        <activity
            android:name=".Loading"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Main Activity -->
        <activity
            android:name=".Mains"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".it_bw"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.codemanager.it_bw_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.codemanager.it_bw_list_2"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".it_bw_login"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity
            android:name=".it_bw_history"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.codecreation.it_bw_cre"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" >
        </activity>
        <activity
            android:name="dev.woody.ext.join.it_bw_reg_person"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity
            android:name="dev.woody.ext.info.it_info"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.info.it_notice"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.info.it_usercenter"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.info.it_qna"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.join.it_bw_reg_yakgwan"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="dev.woody.ext.codecreation.it_bw_cre_date"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="dev.woody.ext.codemanager.it_bw_list_date"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name=".it_bw_find_pw"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.join.it_bw_reg_manage"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.codecreation.it_bw_cre_user"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dev.woody.ext.join.it_bw_reg_password_check"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>

        <!-- 네이버 아이디로 로그인에서 사용하는 액티비티를 등록 -->
        <activity
            android:name="com.nhn.android.naverlogin.ui.OAuthLoginActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.nhn.android.naverlogin.ui.OAuthLoginInAppBrowserActivity"
            android:label="OAuth2.0 In-app" />

        <!-- 페이스북 아이디로 로그인에서 사용하는 액티비티를 등록 -->
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="dev.woody.ext.info.it_yakgwan_1"
            android:label="@string/title_activity_it_yakgwan_1" >
        </activity>
    </application>

</manifest>
